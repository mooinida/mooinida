    "n=int(input())\n",
    "student=[]\n",
    "for i in range(n):\n",
    "    w,h=map(int,input().split())\n",
    "    student.append((w,h))\n",
    "for i in student:\n",
    "    rank=1\n",
    "    for j in student:\n",
    "        if(i[0]<j[0] and i[1]<j[1]):\n",
    "            rank+=1\n",
    "    print(rank,end=' ')"
   ]
