    "n=int(input())\n",
    "all=[]\n",
    "for i in range(n):\n",
    "    x,y=input().split()\n",
    "    x=int(x)\n",
    "    all.append((x,y))\n",
    "c=sorted(all,key=lambda x:(x[0]))\n",
    "for i in c:\n",
    "    print(i[0],i[1])\n"
