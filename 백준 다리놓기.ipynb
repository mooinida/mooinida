    "def facto(n):\n",
    "    f=1\n",
    "    for i in range(1,n+1):\n",
    "         f=f*i\n",
    "    return f\n",
    "n=int(input())\n",
    "for i in range(n):\n",
    "    a,b=map(int,input().split())\n",
    "    result=facto(b)//(facto(a)*facto(b-a))\n",
    "    print(result)"
   ]
