    "n,m=map(int,input().split())\n",
    "s=[]\n",
    "def dfs():\n",
    "    if len(s)==m:\n",
    "        for i in range(m):\n",
    "            print(s[i],end=' ')\n",
    "        print('')\n",
    "        return\n",
    "    for i in range(1,n+1):\n",
    "            s.append(i)\n",
    "            dfs()\n",
    "            s.pop()\n",
    "dfs()"
