    "t=int(input())\n",
    "num=map(int,input().split())\n",
    "prime=0\n",
    "for n in num:\n",
    "    i=0\n",
    "    count=0\n",
    "    while n!=i:\n",
    "        if(n%(n-i)==0):\n",
    "            count+=1\n",
    "        i+=1\n",
    "    if(count==2):\n",
    "        prime+=1\n",
    "print(prime)\n"
   ]
