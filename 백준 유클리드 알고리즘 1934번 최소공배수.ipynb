    "def save(a,b):##최대공약수를 구하는 함수\n",
    "    while b>0:\n",
    "        a,b=b,a%b\n",
    "    return a\n",
    "def mini(a,b):##최소공배수\n",
    "    return a*b//save(a,b)\n",
    "n=int(input())\n",
    "for i in range(n):\n",
    "    a,b=map(int,input().split())\n",
    "    print(mini(a,b))"
   ]
