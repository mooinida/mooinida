    "def facto(n):\n",
    "    a=1\n",
    "    for i in range(1,n+1):\n",
    "        a=a*i\n",
    "    return a\n",
    "m,n=map(int,input().split())\n",
    "result=facto(m)//(facto(n)*facto(m-n))\n",
    "print(result)"
   ]
